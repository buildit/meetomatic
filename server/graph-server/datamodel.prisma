type User {
  id: ID! @unique @id
  name: String!
  email: String!
  password: String!
}

type Card {
  id: ID! @unique @id
  description: String!
  owner: User
  column: String!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}
type Board {
  id: ID! @unique
  name: String!
  password: String!
  owner: User!
}
